{
  "extends": "standard",
  "rules": {
    "quotes": [
      "error",
      "double"
    ],
    "space-before-function-paren": [
      "error",
      "never"
    ],
    "semi": [
      "error",
      "always"
    ]
  },
  "globals": {
    "$": true,
    "chrome": true,
    "FileReader": true,
    "HFX": true,
    "moment": true
  }
}